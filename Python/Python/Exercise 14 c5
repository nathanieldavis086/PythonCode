print("This program takes the velocity and mass of an object and returns the kinetic energy of the object")
def kinetener():
    m=int(input("Please enter the mass of your object: "))
    v=int(input("Please enter the velocity of your object: "))
    KE=0.5*m*v**2
    return KE
print("The kinetic energy of your object is", kinetener())